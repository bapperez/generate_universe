        "rule": "impacto_imediato",
        "guidelines": [
          "iniciar_com_o_ativo_ja_em_suspensao_ou_queda_controlada",
          "nenhuma_introducao_descritiva_previa",
          "acao_tensao_e_detalhe_sensorial_em_primeiro_plano",
          "o_ambiente_aparece_enquanto_o_corpo_se_move"
        ]
      },

